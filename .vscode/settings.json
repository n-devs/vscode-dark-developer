      "editor.tokenColorCustomizations": {
            "textMateRules": [
                  // {
                  //       "name": "HTML Doctype",
                  //       "scope": "meta.tag.sgml.doctype",
                  //       "settings": {
                  //         "foreground": "#994cc3",
                  //         "fontStyle": "italic"
                  //       }
                  //     },
                  {
                        "name": "HTML Tags Name",
                        "scope": [
                              "entity.name.tag"
                        ],
                        "settings": {
                              "foreground": "#16A085"
                        }
                  },
                  {
                        "name": "HTML Attribute Name",
                        "scope": [
                              "entity.other.attribute-name",
                        ],
                        "settings": {
                              "foreground": "#CA6F1E"
                        }
                  },
                  {
                        "name": "HTML Text String Value",
                        "scope": [
                              "string.quoted"
                        ],
                        "settings": {
                              "foreground": "#2980B9"
                        }
                  },
                  {
                        "name": "HTML Punctuation Tag <> </>",
                        "scope": [
                              "punctuation.definition.tag",
                        ],
                        "settings": {
                              "foreground": "#a0a1a7"
                        }
                  },
                  {
                        "name": "HTML Punctuation String",
                        "scope": [
                              "punctuation.definition.string",
                        ],
                        "settings": {
                              "foreground": "#a0a1a7"
                        }
                  },
                  {
                        "name": "HTML Punctuation Comments <!-- -->",
                        "scope": [
                              "punctuation.definition.comment",
                        ],
                        "settings": {
                              "foreground": "#a0a1a7"
                        }
                  },
                  {
                        "name": "HTML Punctuation =",
                        "scope": "punctuation.separator",
                        "settings": {
                              "foreground": "#a0a1a7"
                        }
                  },
            ]
      },
      "html.format.unformattedContentDelimiter": "color"
}