{
  "name": "vscode-dot-developer",
  "displayName": "Dot Developer Theme",
  "icon": "images/dev-icon.png",
  "version": "1.0.4",
  "description": "VS Code Dot Developer Theme ",
  "scripts": {
    "list": "vsce ls",
    "publish": "vsce publish"
  },
  "galleryBanner": {
    "color": "#444444",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n-devs/vscode-dot-developer.git"
  },
  "publisher": "n-devs",
  "keywords": [
    "Theme",
    "Dark Theme",
    "Light Theme"
  ],
  "author": "Nutthapat Piesangka",
  "license": "ISC",
  "engines": {
    "vscode": "^1.33.1"
  },
  "categories": [
    "Themes",
    "Other"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Dot Developer (Dark)",
        "uiTheme": "vs-dark",
        "path": "./themes/Dark-Developer-color-theme.json"
      },
      {
        "label": "Dot Developer (Light)",
        "uiTheme": "vs-light",
        "path": "./themes/Light-Developer-color-theme.json"
      }
    ]
  },
  "devDependencies": {
    "vsce": "^1.69.0"
  }
}
